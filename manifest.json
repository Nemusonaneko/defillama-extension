{
  "name": "DefiLlama",
  "description": "Extension for DefiLlama which shows brief TVL info for a protocol",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./icons/defillama-logo-16.png",
    "32": "./icons/defillama-logo-32.png",
    "64": "./icons/defillama-logo-64.png",
    "128": "./icons/defillama-logo-128.png"
  },
  "action": {
    "default_title": "DefiLlama",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["activeTab"]
}
